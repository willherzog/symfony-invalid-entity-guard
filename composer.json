{
	"name": "willherzog/symfony-invalid-entity-guard",
	"description": "A decorator for the Symfony validator service to prevent an entity which has failed validation from being persisted in its current state.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Will Herzog",
            "email": "willherzog@gmail.com"
        }
    ],
    "require": {
		"php": "^8.2",
		"doctrine/doctrine-bundle": "^2.12",
		"symfony/framework-bundle": "^6.4|^7.0",
		"symfony/validator": "^6.4|^7.0",
		"doctrine/orm": "^2.17"
	},
    "autoload": {
        "psr-4": { "WHSymfony\\WHInvalidEntityGuardBundle\\": "src/" }
    }
}
